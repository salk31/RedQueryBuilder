<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>search</servlet-name>
		<servlet-class>com.redspr.redquerybuilder.sample.SearchServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>search</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>meta</servlet-name>
		<servlet-class>com.redspr.redquerybuilder.sample.MetaServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>meta</servlet-name>
		<url-pattern>/meta</url-pattern>
	</servlet-mapping>
</web-app>
